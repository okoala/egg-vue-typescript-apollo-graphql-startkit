{
  "name": "egg-vue-typescript-apollo-graphql-startkit",
  "version": "0.2.4",
  "files": [
    "dist",
    "app",
    "src"
  ],
  "scripts": {
    "start": "node index.js",
    "dev": "npm-run-all --parallel dev:server dev:client",
    "dev:server": "cross-env NODE_ENV=development nodemon --debug --watch app --watch conf --watch config index.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./build/webpack.client.config.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --progress --hide-modules --config ./build/webpack.server.config.js",
    "test": "npm run test:unit:egg && npm run test:unit:vue",
    "test:e2e": "node test/e2e/runner.js",
    "test:unit:egg": "cross-env VUE_ENV=server TESTS=test/egg/**/*.test.js egg-bin test -r intelli-espower-loader",
    "test:unit:vue": "cross-env BABEL_ENV=development karma start test/vue/karma.unit.config.js",
    "test:cover": "npm run test:cover:egg && npm run test:cover:vue",
    "test:cover:egg": "egg-bin cov -r intelli-espower-loader",
    "test:cover:vue": "cross-env BABEL_ENV=development karma start test/vue/karma.cover.config.js",
    "lint": "npm run lint:egg && npm run lint:vue",
    "lint:egg": "eslint --ext js app/** --fix",
    "lint:vue": "",
    "release": "release-it patch",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "author": "okoala <dapixp@gmail.com>",
  "dependencies": {
    "apollo-client": "^0.4.20",
    "apollo-server": "^0.3.2",
    "axios": "^0.15.0",
    "co": "^4.6.0",
    "deepmerge": "^1.1.0",
    "egg": "^0.2.1",
    "egg-jwt": "^1.1.0",
    "egg-knex": "^1.0.1",
    "egg-view-nunjucks": "^0.5.0",
    "element-ui": "1.0.0-rc.7",
    "es6-promise": "^4.0.5",
    "fast-safe-stringify": "^1.1.0",
    "graphql": "^0.7.1",
    "graphql-tag": "^0.1.14",
    "graphql-tools": "^0.7.2",
    "moment": "^2.14.1",
    "prerender-spa-plugin": "^1.1.0",
